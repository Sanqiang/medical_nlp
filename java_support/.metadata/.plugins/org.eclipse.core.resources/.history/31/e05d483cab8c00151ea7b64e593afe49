package edu.pitt.medical_nlp.graph;

import java.util.ArrayList;

import edu.stanford.nlp.ling.HasWord;

public class WorDNode implements HasWord {

	private static final long serialVersionUID = 2508728633465920904L;
	public int idx;
	public String lemma;
	ArrayList<Edge> links = new ArrayList<>();

	public WorDNode(int idx, String lemma) {
		this.idx = idx;
		this.lemma = lemma;
	}

	@Override
	public String word() {
		return lemma;
	}

	@Override
	public void setWord(String word) {
		this.lemma = word;
		
	}
}
