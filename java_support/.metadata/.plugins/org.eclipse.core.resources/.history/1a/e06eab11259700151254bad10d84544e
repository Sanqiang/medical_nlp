package edu.pitt.medical_nlp.text;

public class Solution {
	public int myAtoi(String str) {
		if (str.length() == 0) {
			return 0;
		}
		boolean sign = true, flagsign = false;
		int digit = 0;
		int total = 0, temp = 0;
		int processed_digit = 0;
		for (int i = 0; i < str.length(); i++) {
			char ch = str.charAt(i);
			if ('0' <= ch && ch <= '9') {
				digit = ch - '0';
				++processed_digit;
			} else if (ch == '+' || ch == '-') {
				if (flagsign) {
					return 0;
				}
				sign = ch == '+' ? true : false;
				flagsign = true;

			} else if (ch == ' ') {
				continue;
			} else {
				break;
			}
			temp = total * 10 + digit;
			if ( || (temp - digit) / 10d != total || (temp < 0 && total > 0)) {
				return sign ? Integer.MAX_VALUE : Integer.MIN_VALUE;
			} else {
				total = temp;
				
			}
		}
		total = total * (sign ? 1 : -1);

		return total;
	}

	public static void main(String[] args) {
		int total = new Solution().myAtoi("2147483648");
		System.out.println(total);
	}

}