import edu.stanford.nlp.ling.HasWord;
import edu.stanford.nlp.process.DocumentPreprocessor;

public class PostProcess extends Process {
	public PostProcess() {
		super();
		processDocs();
	}

	public void processDocs() {
		for (String doc : this._docs) {
			processDocs(doc);
		}
	}

